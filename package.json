{
  "name": "create-api-hook",
  "version": "1.0.3",
  "description": "一个功能强大的基于 React Hooks 的 API 请求库，提供简洁的 API 调用接口、拦截器、缓存、重试、防抖节流等功能",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "type-check": "tsc --noEmit",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "prepublishOnly": "npm run build && npm run test",
    "prepare": "npm run build"
  },
  "keywords": [
    "react",
    "hooks",
    "api",
    "http",
    "request",
    "fetch",
    "typescript",
    "axios",
    "interceptor",
    "cache",
    "retry",
    "debounce",
    "throttle",
    "batch-request"
  ],
  "author": "PigPiggo",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@types/jest": "^29.0.0",
    "@types/react": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.0.0",
    "rollup": "^3.0.0",
    "rollup-plugin-typescript2": "^0.34.0",
    "ts-jest": "^29.0.0",
    "typescript": "^4.9.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/PigPiggo/create-api-hook.git"
  },
  "bugs": {
    "url": "https://github.com/PigPiggo/create-api-hook/issues"
  },
  "homepage": "https://github.com/PigPiggo/create-api-hook#readme",
  "engines": {
    "node": "18.19.0"
  }
}
